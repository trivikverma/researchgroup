@incollection{albeverio_fifty_2008,
  title = {Fifty {{Years}} of {{Urban Modeling}}: {{Macro}}-{{Statics}} to {{Micro}}-{{Dynamics}}},
  shorttitle = {Fifty {{Years}} of {{Urban Modeling}}},
