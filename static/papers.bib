  file = {/Users/trivikverma/Dropbox/Academics/Zotero/storage/BEBF4UCP/Gao, Barzel, Barabási - 2016 - Universal resilience patterns in complex networks(2).pdf;/Users/trivikverma/Dropbox/Academics/Zotero/storage/ZM2J5ER4/Gao et al. - 2016 - Universal resilience patterns in complex networks.pdf},
