@article{Immerzeel2014,
  title = {High-Resolution Monitoring of {{Himalayan}} Glacier Dynamics Using Unmanned Aerial Vehicles},
  author = {Immerzeel, W. W. and Kraaijenbrink, P. D.A. and Shea, J. M. and Shrestha, A. B. and Pellicciotti, F. and Bierkens, M. F.P. and De Jong, S. M.},
